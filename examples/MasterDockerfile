FROM ubuntu


ENV DEBIAN_FRONTEND=noninteractive


# install packages
RUN apt-get update -y && apt-get install -y \
\
    gcc \
    git \
 \
&& apt-get clean \
&& rm -rf /var/lib/apt/lists/* /tmp/* /var/tmp/*


